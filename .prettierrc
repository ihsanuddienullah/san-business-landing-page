{
  "semi": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "printWidth": 120,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss",
    "prettier-plugin-classnames"
  ],
  "importOrder": [
    "^react$",
    "^react/(.*)$",
    "^[^./]",
    "^/(.*)$",
    "^[./].*(?<!\\.(css|scss|sass|less))$",
    "\\.(css|scss|sass|less)$"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true
}